code 5: Fibonacci Number
let fib=N=>{
  return (N<2)?N:fib(N-2)+fib(N-1);
}
console.log(fib(7))   //output:13
----------------------------------------------------------------------------
const fib=n=>{
  const memo={}
  for(let i=0;i<=n;i++){
    if(i<2){
      memo[i]=i
    }else{
      memo[i]=memo[i-2]+memo[i-1]
    }
  }
  return memo[n]
}
console.log(fib(7))    //output:13
-----------------------------------------------------------------------------
const fib=n=>{
  let arr=[0,1]
  for(let i=2;i<=n;i++){
    arr.push(arr[i-2]+arr[i-1])
  }
  return arr[n];
}
console.log(fib(7))   //output:13
